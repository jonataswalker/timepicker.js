{
    "name": "timepicker.js",
    "version": "3.1.0",
    "description": "A lightweight, customizable, TimePicker",
    "author": "Jonatas Walker",
    "homepage": "https://github.com/jonataswalker/timepicker.js",
    "license": "MIT",
    "type": "module",
    "main": "./dist/timepicker.umd.cjs",
    "jsdelivr": "./dist/timepicker.iife.js",
    "module": "./dist/timepicker.js",
    "exports": {
        ".": {
            "import": "./dist/timepicker.js",
            "require": "./dist/timepicker.umd.cjs",
            "types": "./dist/types.d.ts"
        }
    },
    "files": ["dist"],
    "types": "./dist/types.d.ts",
    "engines": {
        "node": "^16 || ^18 || ^20",
        "npm": ">=8"
    },
    "scripts": {
        "dev": "vite",
        "build": "rimraf ./dist && vite build",
        "lint": "eslint .",
        "lint:fix": "npm run lint -- --fix",
        "prepublishOnly": "npm run lint && npm test && npm run build",
        "prepare": "husky",
        "test": "node bin/test"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/jonataswalker/timepicker.js.git"
    },
    "bugs": {
        "url": "https://github.com/jonataswalker/timepicker.js/issues"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/node": "^20.11.24",
        "eslint-config-jwalker": "^10.1.1",
        "husky": "^9.0.11",
        "rimraf": "^5.0.5",
        "sass": "^1.71.1",
        "typescript": "^5.3.3",
        "vite": "^5.1.4",
        "vite-plugin-banner": "^0.7.1",
        "vite-plugin-css-injected-by-js": "^3.4.0",
        "vite-plugin-dts": "^3.7.3"
    },
    "dependencies": {
        "mitt": "^3.0.1"
    }
}
